{
  "manifest_version": 3,
  "name": "ReVision: Fighting Bias",
  "description": "A proof-reading app that will help you identify and eliminate bias",
  "version": "0.0.1",
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
