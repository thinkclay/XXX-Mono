{
  "manifest_version": 3,
  "name": "ReVision: Fighting Bias",
  "short_name": "ReVision",
  "description": "A proof-reading app that will help you identify and eliminate bias",
  "version": "0.1.0",
  "author": "Clay Unicorn <thinkclay@gmail.com>",
  "oauth2": {
    "client_id": "901177621766-s7e5fe6dbm9o2m56iks5a2t076vrkqou.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/userinfo.email"]
  },
  "content_security_policy": {
    "extension_pages": "connect-src 'self' https://apis.google.com; object-src 'self'"
  },
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["identity"]
}
