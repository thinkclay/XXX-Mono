{
  "manifest_version": 3,
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "content_security_policy": {
    "extension_pages": "connect-src 'self' https://apis.google.com; object-src 'self'"
  },
  "host_permissions": ["https://*/*"],
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["identity"]
}
